{
  "rules": [
    {
      "description": "Simple Test Rule",
      "backend": {
        "fqdn": "localhost",
        "port": 3000,
        "protocol": 0
      },
      "frontend": {
        "fqdn": "simple.example.com",
        "port": 443,
        "protocol": 1,
        "https": {
          "hsts": false
        },
        "acl": null
      },
      "customize_headers": [
        {
          "name": "X-Forwarded-For",
          "value": "$remote_addr"
        }
      ],
      "proxy_connect_timeout": 60,
      "proxy_read_timeout": 60,
      "proxy_send_timeout": 60,
      "proxy_http_version": 1,
      "proxy_intercept_errors": false
    }
  ]
}

